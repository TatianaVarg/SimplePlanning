<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simpleplanning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimplePlanning">
        <activity android:name=".AddToDoMonth"></activity>
        <activity android:name=".ToDoMonth" />
        <activity android:name=".AddToBuy" />
        <activity android:name=".ToBuyList" />
        <activity android:name=".AddToDo" />
        <activity android:name=".ToDoList" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>